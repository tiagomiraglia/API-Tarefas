{
  "typescript.preferences.includePackageJsonAutoImports": "auto",
  "typescript.suggest.autoImports": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "typescript.preferences.includeCompletionsForImportStatements": true,
  "typescript.preferences.includeCompletionsWithInsertText": true,
  "typescript.preferences.includeCompletionsForModuleExports": true,
  "typescript.suggest.includeCompletionsForImportStatements": true,
  "typescript.preferences.useAliasesForRenames": true,
  "typescript.preferences.renameShorthandProperties": false,
  "typescript.preferences.allowRenameOfImportPath": true,
  "typescript.preferences.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.includeInlayParameterNameHints": "all",
  "typescript.preferences.includeInlayParameterNameHintsWhenArgumentMatchesName": false,
  "typescript.preferences.includeInlayFunctionParameterTypeHints": true,
  "typescript.preferences.includeInlayVariableTypeHints": true,
  "typescript.preferences.includeInlayPropertyDeclarationTypeHints": true,
  "typescript.preferences.includeInlayFunctionLikeReturnTypeHints": true,
  "typescript.preferences.includeInlayEnumMemberValueHints": true,
  "typescript.preferences.noSemicolons": "ignore",
  "typescript.preferences.quoteStyle": "single",
  "typescript.preferences.importModuleSpecifierEnding": "minimal",
  "typescript.preferences.jsxAttributeCompletionStyle": "auto",
  "typescript.preferences.includeCompletionsWithClassMemberSnippets": true,
  "typescript.preferences.includeCompletionsWithObjectLiteralMethodSnippets": true,
  "typescript.preferences.autoImportFileExcludePatterns": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**"
  ],
  "typescript.validate.enable": true,
  "typescript.tsc.autoDetect": "on",
  "typescript.check.npmIsInstalled": false,
  "typescript.preferences.tsconfig": "./tsconfig.json"
}